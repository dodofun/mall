{
  "name": "mall",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "taro-ui",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:h5:mock": "npm run build:h5 MOCK=true -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,scss,css,md}'",
    "eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "stylelint": "stylelint src/**/*.{js,jsx,tsx,ts,scss,css} --fix",
    "fix": "npm run eslint && npm run stylelint"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/cli": "3.0.21",
    "@tarojs/components": "3.0.21",
    "@tarojs/react": "3.0.21",
    "@tarojs/runtime": "3.0.21",
    "@tarojs/taro": "3.0.21",
    "ahooks": "^2.9.2",
    "dayjs": "^1.9.7",
    "hox": "^1.1.2",
    "lodash": "^4.17.20",
    "mockjs": "^1.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "taro-code": "^3.2.0",
    "taro-plugin-canvas": "^1.0.10",
    "taro-ui": "^3.0.0-alpha.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.0.21",
    "@tarojs/webpack-runner": "3.0.21",
    "@types/react": "^17.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "^10.1.0",
    "babel-preset-taro": "3.0.21",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-taro": "3.0.21",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.3"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts,scss,css,md}": [
      "npm run fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
